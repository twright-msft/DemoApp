            <th>
                @Html.DisplayNameFor(model => model.PitchAngle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vibration)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TurbineSpeedAverage)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AlterBlades)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.GearboxOilLevel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GearboxOilTemp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GeneratorActivePower)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GeneratorSpeed)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GeneratorTorque)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GridFrequency)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GridVoltage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HydraulicOilPressure)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NacelleAngle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OverallWindDirection)
            </td>
