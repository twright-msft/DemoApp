@model IEnumerable<DemoApp.Models.TurbineTelemetrySample>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.GearboxOilLevel)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GearboxOilTemp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GeneratorActivePower)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GeneratorSpeed)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GeneratorTorque)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GridFrequency)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GridVoltage)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HydraulicOilPressure)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NacelleAngle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OverallWindDirection)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WindSpeedStdDev)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Precipitation)
